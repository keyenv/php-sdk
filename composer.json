{
    "name": "keyenv/keyenv",
    "description": "Official PHP SDK for KeyEnv - Secure secrets management",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "keyenv",
        "secrets",
        "environment",
        "env",
        "dotenv",
        "secrets-management",
        "configuration"
    ],
    "authors": [
        {
            "name": "KeyEnv",
            "email": "support@keyenv.dev"
        }
    ],
    "homepage": "https://keyenv.dev",
    "support": {
        "issues": "https://github.com/keyenv/php-sdk/issues",
        "source": "https://github.com/keyenv/php-sdk"
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "KeyEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeyEnv\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=8"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
